{
  "name": "react-pp",
  "version": "1.1.7",
  "description": "Useful React Hooks",
  "main": "src/index.js",
  "scripts": {
    "test": "jest",
    "test:coverage": "jest --coverage",
    "format": "prettier --single-quote --trailing-comma=none --write 'src/**/**.js' 'src/**.js'",
    "format:verify": "prettier --single-quote --trailing-comma=none --list-different 'src/**/**.js' 'src/**.js'",
    "lint": "eslint 'src/**/**.js' 'src/**.js'",
    "lint:fix": "eslint --fix 'src/**/**.js' 'src/**.js'"
  },
  "keywords": [
    "react",
    "hooks"
  ],
  "jest": {
    "coverageThreshold": {
      "global": {
        "functions": 90,
        "lines": 90,
        "statements": 90
      }
    },
    "transform": {
      "^.+\\.(js)$": "babel-jest"
    }
  },
  "repository": {
    "url": "https://github.com/dosullivan557/react-pp",
    "directory": "src",
    "type": "github"
  },
  "author": "Danny O'Sullivan",
  "license": "ISC",
  "peerDependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.20.12",
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-react": "^7.18.6",
    "@testing-library/react": "^13.4.0",
    "babel-jest": "^29.4.1",
    "eslint": "^8.33.0",
    "eslint-plugin-react": "^7.32.2",
    "jest": "^29.4.1",
    "jest-environment-jsdom": "^29.4.1",
    "prettier": "^2.8.3"
  }
}
